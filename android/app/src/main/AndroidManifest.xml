<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.setting.password.ChangePasswordActivity"></activity>
        <activity android:name=".ui.profile.edit.EditProfileActivity" />
        <activity android:name=".ui.log.ActiveLogActivity" />
        <activity android:name=".ui.profile.profile.ProfileActivity" />
        <activity android:name=".ui.main.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.LoginActivity" />
        <activity android:name=".ui.active.add.AddActiveActivity" />
        <activity android:name=".ui.active.list.activity.ListActiveActivity" />
        <activity android:name=".ui.active.info.InformationActivity" />
        <activity android:name=".ui.active.confirm.ConfirmActivity" />
    </application>

</manifest>